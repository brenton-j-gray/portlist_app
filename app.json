{
  "expo": {
    "name": "Portlist",
    "slug": "portlist",
    "version": "1.0.1",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "splash": {
      "image": "./assets/images/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#0A2540"
    },
    "scheme": "portlist",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": false,
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "NSPhotoLibraryUsageDescription": "Allow access to select photos for your notes.",
        "NSPhotoLibraryAddUsageDescription": "Allow saving photos you take to your Photo Library.",
        "NSCameraUsageDescription": "Allow taking photos for your notes.",
        "NSLocationWhenInUseUsageDescription": "Allow using your location to tag notes with a place.",
        "ITSAppUsesNonExemptEncryption": false
      },
      "bundleIdentifier": "com.sydian.portlist"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#0A2540"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.sydian.portlist",
      "permissions": [
        "CAMERA",
        "READ_MEDIA_IMAGES",
        "ACCESS_COARSE_LOCATION",
        "ACCESS_FINE_LOCATION"
      ]
    },
    "plugins": [
      "expo-secure-store",
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#0A2540"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "EXPO_PUBLIC_API_URL": "http://192.168.4.114:4000",
      "eas": {
        "projectId": "9cacb25f-e73d-4a0a-97fa-bcdf85e66c37"
      }
    },
    "owner": "portlist-team"
  }
}
